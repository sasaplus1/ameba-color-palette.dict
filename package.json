{
  "devDependencies": {
    "ameba-color-palette.css": "^4.8.0",
    "css-tree": "^1.1.3"
  },
  "private": true,
  "scripts": {
    "attach": "git worktree add ./dist master",
    "build": "node ./npm_scripts/extract.js > ./dict/ameba-color-palette.dict",
    "copy": "cp -rf ./dict ./dist",
    "detach": "git worktree remove ./dist",
    "postrelease": "yarn run detach",
    "prerelease": "yarn run build && yarn run attach",
    "release": "yarn run copy && (cd ./dist && git commit -am release)"
  }
}
